#
LoadPlugin "<%= @name %>"

<% if not @config.empty? %>
<Plugin "<%= @name %>">
  <%= "<Carbon>" if @name == "write_graphite" %>
  <%= "<Instance \"apache\">" if @name == "apache" %>
  <%= "<URL #{collectd_option(@config.delete("URL"))}>" if @name == "write_http" %>
  <% @config.each_pair do |key, value|
    if value.is_a? Array
      value.each do |subvalue| %>
  <%= key %> <%= collectd_option(subvalue) %>
  <% end else %>
  <%= key %> <%= collectd_option(value) %>
  <% end end %>
  <%= "</Carbon>" if @name == "write_graphite" %>
  <%= "</Instance>" if @name == "apache" %>
  <%= "</URL >" if @name == "write_http" %>
</Plugin>
<% end %>
